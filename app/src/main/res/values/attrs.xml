<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerticalSeekBar">
        <attr name="seekBarRotation">
            <!-- Clock wise - 90 deg; top = min, bottom = max -->
            <enum name="CW90" value="90" />
            <!-- Clock wise - 270 deg; bottom = min, top = max -->
            <enum name="CW270" value="270" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SwipeViewPager">
        <attr name="swipeEnable" format="reference|boolean" />
    </declare-styleable>
    <declare-styleable name="MarqueeView">

        <attr name="marqueeview_repet_type" format="enum">
            <enum name="repet_oncetime" value="0"/>
            <enum name="repet_interval" value="1"/>
            <enum name="repet_continuous" value="2"/>
        </attr>

        <attr name="marqueeview_text_distance" format="integer"/>
        <attr name="marqueeview_text_startlocationdistance" format="float"/>
        <attr name="marqueeview_text_speed" format="float"/>
        <attr name="marqueeview_text_color" format="color|reference"/>
        <attr name="marqueeview_text_size" format="float"/>

        <attr name="marqueeview_isclickalbe_stop" format="boolean"/>
        <attr name="marqueeview_is_resetLocation" format="boolean"/>



    </declare-styleable>
    <declare-styleable name="stl_SmartTabLayout">
        <attr name="stl_indicatorAlwaysInCenter" format="boolean"/>
        <attr name="stl_indicatorWithoutPadding" format="boolean"/>
        <attr name="stl_indicatorInFront" format="boolean"/>
        <attr name="stl_indicatorInterpolation" format="enum">
            <enum name="smart" value="0"/>
            <enum name="linear" value="1"/>
        </attr>
        <attr name="stl_indicatorGravity" format="enum">
            <enum name="bottom" value="0"/>
            <enum name="top" value="1"/>
            <enum name="center" value="2"/>
        </attr>
        <attr name="stl_indicatorColor" format="color"/>
        <attr name="stl_indicatorColors" format="reference"/>
        <attr name="stl_indicatorThickness" format="dimension"/>
        <attr name="stl_indicatorWidth" format="dimension">
            <enum name="auto" value="-1"/>
        </attr>
        <attr name="stl_indicatorCornerRadius" format="dimension"/>
        <attr name="stl_overlineColor" format="color"/>
        <attr name="stl_overlineThickness" format="dimension"/>
        <attr name="stl_underlineColor" format="color"/>
        <attr name="stl_underlineThickness" format="dimension"/>
        <attr name="stl_dividerColor" format="color"/>
        <attr name="stl_dividerColors" format="reference"/>
        <attr name="stl_dividerThickness" format="dimension"/>
        <attr name="stl_defaultTabBackground" format="reference"/>
        <attr name="stl_defaultTabTextAllCaps" format="boolean"/>
        <attr name="stl_defaultTabTextColor" format="color|reference"/>
        <attr name="stl_defaultTabTextSize" format="dimension"/>
        <attr name="stl_defaultTabTextHorizontalPadding" format="dimension"/>
        <attr name="stl_defaultTabTextMinWidth" format="dimension"/>
        <attr name="stl_customTabTextLayoutId" format="reference"/>
        <attr name="stl_customTabTextViewId" format="reference"/>
        <attr name="stl_distributeEvenly" format="boolean"/>
        <attr name="stl_clickable" format="boolean"/>
        <attr name="stl_titleOffset" format="dimension">
            <enum name="auto_center" value="-1"/>
        </attr>
        <attr name="stl_drawDecorationAfterTab" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ShadowView">
        <attr name="shadowDx" format="dimension" />
        <attr name="shadowDy" format="dimension" />
        <attr name="shadowRadius" format="dimension" />
        <attr name="shadowAlpha" format="integer" />
    </declare-styleable>


    <declare-styleable name="Croller">
        <attr name="enabled" format="boolean" />
        <attr name="start_progress" format="integer" />
        <attr name="label" format="string" />
        <attr name="back_circle_color" format="color" />
        <attr name="back_circle_disable_color" format="color" />
        <attr name="main_circle_color" format="color" />
        <attr name="main_circle_disable_color" format="color" />
        <attr name="indicator_color" format="color" />
        <attr name="indicator_disable_color" format="color" />
        <attr name="progress_primary_color" format="color" />
        <attr name="progress_primary_disable_color" format="color" />
        <attr name="progress_secondary_color" format="color" />
        <attr name="progress_secondary_disable_color" format="color" />
        <attr name="label_size" format="dimension" />
        <attr name="label_color" format="color" />
        <attr name="label_font" format="string" />
        <attr name="label_style" format="enum" >
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2"/>
            <enum name="bold_italic" value="3"/>
        </attr>
        <attr name="label_disabled_color" format="color" />
        <attr name="indicator_width" format="float" />
        <attr name="is_continuous" format="boolean" />
        <attr name="progress_primary_circle_size" format="float" />
        <attr name="progress_secondary_circle_size" format="float" />
        <attr name="progress_primary_stroke_width" format="float" />
        <attr name="progress_secondary_stroke_width" format="float" />
        <attr name="sweep_angle" format="integer" />
        <attr name="start_offset" format="integer" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="main_circle_radius" format="float" />
        <attr name="back_circle_radius" format="float" />
        <attr name="progress_radius" format="float" />
        <attr name="anticlockwise" format="boolean" />
    </declare-styleable>


    <declare-styleable name="TemplateView">
        <attr name="gnt_template_type" format="reference"/>
    </declare-styleable>
    <declare-styleable name="BannerLayout">
        <attr name="moveSpeed" format="float" />
        <attr name="centerScale" format="float" />



        <attr name="itemSpace" format="integer" />
    </declare-styleable>
    <declare-styleable name="RecyclerViewBannerBase">
        <attr name="indicatorSpace" format="reference|dimension" />
        <attr name="indicatorType">
            <enum name="circular" value="1" />
            <enum name="custom" value="2" />
            <enum name="linear" value="0" />
        </attr>
        <attr name="orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="indicatorMarginBottom" format="reference|dimension" />
        <attr name="indicatorMarginLeft" format="reference|dimension" />
        <attr name="indicatorMarginRight" format="reference|dimension" />
        <attr name="indicatorGravity">
            <enum name="center" value="1" />
            <enum name="left" value="0" />
            <enum name="right" value="2" />
        </attr>
        <attr name="autoPlaying" format="boolean" />
        <attr name="indicatorSelectedSrc" format="reference|color" />
        <attr name="interval" format="integer" />
        <attr name="showIndicator" format="boolean" />
        <attr name="indicatorUnselectedSrc" format="reference|color" />

    </declare-styleable>


</resources>